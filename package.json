{
  "name": "node-sass-template",
  "version": "0.0.1",
  "description": "sass development with node.js environment",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:sass": "node-sass --watch sass --output-style expanded --recursive sass --output dist/css",
    "dev:scss": "node-sass --watch scss --output-style expanded --recursive scss --output dist/css",
    "build:sass": "node-sass --output-style compressed sass/main.sass -o dist/sass && cp dist/sass/main.css dist/css",
    "build:scss": "node-sass --output-style compressed scss/main.scss -o dist/scss && cp dist/sass/main.css dist/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmyferiawan/node-sass-template.git"
  },
  "keywords": [
    "sass"
  ],
  "author": "jimmy feriawan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmyferiawan/node-sass-template/issues"
  },
  "homepage": "https://github.com/jimmyferiawan/node-sass-template#readme",
  "devDependencies": {
    "node-sass": "^4.9.2"
  },
  "dependencies": {}
}
